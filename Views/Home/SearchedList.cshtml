@{
    ViewBag.Title = "Searched Movie List";
}

@model Assignment3.Models.ApplicationResponse

<div>
    <h2>Here are your movie search history: </h2>
    <p>Category: @Model.Category</p>
    <p>Title: @Model.Title</p>
    <p>Year: @Model.Year</p>
    <p>Director: @Model.Director</p>
    <p>Rating: @Model.Rating</p>
    <p>Edited: @Model.Edited</p>
    <p>Lent To: @Model.LentTo</p>
    <p>Notes: @Model.Notes</p>
</div>
